#!/bin/bash

make clean
rm -rf `find . -name CMakeCache.txt` \
       `find . -name CMakeFiles` \
       `find . -name CTestTestfile.cmake` \
       `find . -name Makefile` \
       `find . -name cmake_install.cmake` \
       spim/parser-yacc.c \
       spim/parser-yacc.h \
       spim/parse-yacc.output \
       spim/scanner.c

